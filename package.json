{
  "name": "scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build:js": "webpack",
    "watch:js": "webpack --watch",
    "dev": "npm run watch:js & npm run watch:css &  npm run serve & npm start",
    "postinstall": "cp node_modules/normalize.css/normalize.css lib/",
    "test": "mocha --require ./test/setup.js ./test/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "css": "node-sass src/scss/main.scss -o dist",
    "watch:css": "node-sass src/scss/main.scss -wo dist",
    "serve": "browser-sync start --proxy 'http://localhost:3000' --files 'dist/*.css, js/*.js'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "browser-sync": "^2.18.2",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "event-emitter": "^0.3.4",
    "eventemitter3": "^2.0.2",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "lodash": "^4.16.6",
    "lowdb": "^0.14.0",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "shortid": "^2.2.6",
